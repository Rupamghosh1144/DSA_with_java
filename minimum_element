public class Main{
    static void min_value(int[] arr){
        int n=arr.length;
        int ans=arr[0];
        for(int i=0;i<n-1;i++){
            if(arr[i]<ans){
                ans=arr[i];
            }
        }
        System.out.println(ans);
    }
    public static void main(String[] args){
        int arr[]={0,2,4,3,55,33,22,12};
        min_value(arr);
    }
}
