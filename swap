public class Main{
    static void swap(int a, int b){
        int temp=a;
        a=b;
        b=temp;
        System.out.println("After swap::-->a is: "+a+" ans b is: "+b);
    }
    public static void main(String[] args){
        int a=5,b=10;
        System.out.println("Before swap::-->a is: "+a+" ans b is: "+b);
        swap(a,b);
    }
}
