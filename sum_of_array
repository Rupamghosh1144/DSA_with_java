//sum of the array
public class Main{
    static void sum_arr(int[] arr){
        int sum=0;
        for(int i=0;i<arr.length;i++){
           sum+=arr[i]; 
        }
        System.out.println("sum of the array is: "+sum);
    }
    
    public static void main(String[] args){
        int arr[]={1,2,3,10,4};
        sum_arr(arr);
        
    }
}
